{
  "name": "where-should-we-be",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production kaliber-build",
    "watch": "CONFIG_ENV=dev kaliber-watch",
    "serve": "kaliber-serve",
    "serve.dev": "PORT=8000 CONFIG_ENV=dev kaliber-serve",
    "lint": "kaliber-lint",
    "start": "run-p prettier-watch watch serve.dev",
    "prettier-watch": "onchange './src/**/*.js' './config/**/*.js' -- prettier --write {{changed}}"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "npm-run-all": "^4.1.3",
    "onchange": "5.2.0",
    "prettier": "1.17.0"
  },
  "dependencies": {
    "@kaliber/build": "*",
    "cross-browser-resize": "^1.0.3",
    "d3-array": "^2.0.3",
    "d3-scale": "^3.0.0",
    "d3-scale-chromatic": "^1.3.3",
    "d3-shape": "^1.3.5",
    "firebase": "^5.11.0",
    "firebase-admin": "^7.3.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "three": "^0.104.0"
  }
}
